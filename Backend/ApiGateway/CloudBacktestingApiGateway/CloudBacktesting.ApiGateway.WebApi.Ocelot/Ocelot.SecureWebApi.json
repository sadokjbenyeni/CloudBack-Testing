{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/user/isLogin",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/isLogin",
      "ServiceName": "nodeservices"
    },
    {
      "DownstreamPathTemplate": "/api/user/logout",
      "DownstreamScheme": "http",
      "ServiceName": "nodeservices",
      "UpstreamPathTemplate": "/api/logout",
      "UpstreamHttpMethod": [ "Post" ],
      "RouteClaimsRequirement": {
        "IsLogin": "True",
        "State": "1"
      }

    },
    {
      "DownstreamPathTemplate": "/api/role/{url}",
