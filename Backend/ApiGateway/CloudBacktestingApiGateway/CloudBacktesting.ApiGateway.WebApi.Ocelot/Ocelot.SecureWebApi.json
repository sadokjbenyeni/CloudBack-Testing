      "UpstreamPathTemplate": "/api/user/info",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "RouteClaimsRequirement": {
        "IsLogin": "True",
        "State": "1"

      }
    },
    //currency
    {
      "DownstreamPathTemplate": "/api/currency",
      "DownstreamScheme": "http",
      "ServiceName": "nodeservices",
      "UpstreamPathTemplate": "/api/currency",
