      "UpstreamHttpMethod": [ "GET" ],
      "RouteClaimsRequirement": {
        "Role": "Administrator",
        "IsLogin": "True",
        "State": "1"
      }
    },
    {
      "DownstreamPathTemplate": "/api/role",
      "DownstreamScheme": "http",
      "ServiceName": "nodeservices",
      "UpstreamPathTemplate": "/api/role",
      "UpstreamHttpMethod": [ "GET" ],
      "RouteClaimsRequirement": {
        "Role": "Administrator",
        "IsLogin": "True",
        "State": "1"
      }
    },
    {
